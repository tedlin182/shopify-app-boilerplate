{
  "root": true,
  "parser": "babel-eslint",
  "globals": {
    "React": "writable"
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:jsx-a11y/recommended",
    "plugin:react-hooks/recommended",
    "airbnb"
  ],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ],
    "arrow-parens": 0,
    "function-paren-newline": 0,
    "implicit-arrow-linebreak": 0,
    "import/no-named-as-default": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "no-confusing-arrow": 0,
    "operator-linebreak": 0,
    "indent": 0,
    "semi": 0,
    "max-len": 0,
    "no-console": 0,
    "react/react-in-jsx-scope": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-indent": 0,
    "quotes": [
      "error",
      "single"
    ]
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "plugins": []
}
